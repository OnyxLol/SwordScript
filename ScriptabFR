gui1=Instance.new("BillboardGui")
gui1.Parent = game.Players.LocalPlayer.Character.Torso
gui1.Adornee = game.Players.LocalPlayer.Character.Torso
gui1.Size=UDim2.new(6.5,0,6.5,0)
gui1.StudsOffset=Vector3.new(0,0.2,0)
gui1.AlwaysOnTop = false
text1=Instance.new("ImageLabel")
text1.Image = "http://www.roblox.com/asset/?id=452859983" --Anime girl asset here
text1.Size=UDim2.new(1,0,1,0)
text1.Position=UDim2.new(0,0,0,0)
text1.BackgroundTransparency = 1
text1.Parent=gui1
for a, obj in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
if obj:IsA("BasePart") then obj.Transparency = 1 if obj:findFirstChild("face") then obj.face.Transparency = 1 end elseif obj:IsA("Hat") and obj:findFirstChild("Handle") then obj.Handle.Transparency = 1 end
end
